{
  "id": null,
  "uid": "medistream-dashboard",
  "title": "Medistream Backend Metrics",
  "tags": ["backend", "prometheus", "go"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "HTTP Request Duration (seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "{{endpoint}} - {{method}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "DB Query Latency (seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "rate(db_query_latency_total_sum[1m]) / rate(db_query_latency_total_count[1m])",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Cache Hits & Misses",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "rate(cache_hits_total[1m])",
          "legendFormat": "Cache Hit - {{cache_key}}",
          "refId": "A"
        },
        {
          "expr": "rate(cache_misses_seconds[1m])",
          "legendFormat": "Cache Miss - {{cache_key}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Cache Hit Ratio",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "rate(cache_hits_total[1m]) / (rate(cache_hits_total[1m]) + rate(cache_misses_seconds[1m]))",
          "legendFormat": "{{cache_key}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s",
  "schemaVersion": 37,
  "version": 1,
  "editable": true
}